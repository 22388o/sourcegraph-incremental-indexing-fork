{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}", "--allow-insecure-localhost", "/home/sqs/src/github.com/sourcegraph/sourcegraph.tmp", "/home/sqs/src/github.com/hashicorp/go-multierror"],
			"outFiles": ["${workspaceFolder}/out/**/*.js"],
			"preLaunchTask": "npm: watch"
		}
	]
}
