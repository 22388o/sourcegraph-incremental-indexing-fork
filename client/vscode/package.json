{
  "name": "sourcegraph-explore-usage",
  "description": "Sourcegraph - Explore usage",
  "version": "0.0.1",
  "publisher": "sourcegraph",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sourcegraph.showExploreUsage",
    "onView:sourcegraph.exploreUsage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Explore Usage",
        "command": "sourcegraph.showExploreUsage"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "sourcegraph.showExploreUsage"
        }
      ],
      "editor/context": [
        {
          "command": "sourcegraph.showExploreUsage",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "sourcegraph.exploreUsage",
          "name": "Explore Usage"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -w -p ./"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "typescript": "^4.3.2"
  }
}
