{
  "private": true,
  "name": "@sourcegraph/vscode",
  "displayName": "Sourcegraph",
  "version": "0.0.1",
  "description": "Sourcegraph VS Code extension",
  "publisher": "sourcegraph",
  "sideEffects": false,
  "license": "Apache-2.0",
